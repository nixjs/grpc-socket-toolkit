{
  "name": "@nixjs23n6/grpc-socket-react",
  "version": "0.0.13",
  "description": "A react context which helps you to deal with gRPC websocket.",
  "sideEffects": false,
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "browser": "lib/esm/index.js",
  "types": "lib/types/index.d.ts",
  "keywords": [
    "grpc",
    "grpc-web",
    "grpc-client",
    "grpc-web-client",
    "grpc-client-toolkit",
    "grpc-toolkit",
    "toolkit",
    "protobuf",
    "typescript",
    "web"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nixjs/grpc-socket-toolkit.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "url": "https://github.com/nixjs",
    "name": "nghinv"
  },
  "license": "MIT",
  "files": [
    "lib"
  ],
  "scripts": {
    "clean": "rimraf lib/*",
    "build:pre": "run-s clean",
    "build:cjs": "npx tsc -p tsconfig.cjs.json",
    "build:esm": "npx tsc -p tsconfig.esm.json",
    "build": "run-s build:pre build:cjs build:esm",
    "lint": "eslint 'src/**/*.{ts,tsx}'"
  },
  "peerDependencies": {
    "@nixjs23n6/grpc-socket-core": "^0.0.13",
    "@nixjs23n6/types": "^1.0.3",
    "react": ">=16"
  },
  "devDependencies": {
    "@nixjs23n6/grpc-socket-core": "^0.0.13",
    "@nixjs23n6/types": "^1.0.3",
    "react": "^17.0.0"
